        <msapplication>
            <tile>
                <square70x70logo src="{{asset('img/Logos_ljm/favicon/smalltile.png')}}">
                    <square150x150logo src="{{asset('img/Logos_ljm/favicon/mediumtile.png')}}">
                        <square310x310logo src="{{asset('img/Logo_ljm/favicon/largetile.png')}}">

            </tile>
        </msapplication>
